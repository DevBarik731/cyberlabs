{
  "manifest_version": 3,
  "name": "TextMate",
  "version": "1.0",
  "description": "Smart Reading Web-Assistant",
  "permissions": [
    "storage"
  ],
  "host_permissions":[
    "https://generativelanguage.googleapis.com/*",
    "https://openrouter.ai/api/v1/chat/completions"
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "css": ["style.css"]
    }
  ]
}

